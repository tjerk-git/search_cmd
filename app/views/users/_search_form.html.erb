<%= form_with url: search_users_path, method: :post, data: {controller: "debounce", debounce_target: "form" } do |form| %>
  <div class="field">
    <div class="control">
      <%= form.search_field :name_search, data: { action: "input->debounce#search" }, placeholder: "Search CMDLWD by name, skill, expertise or minor..." %>
    </div>
  </div>
<% end %>

<div id="search_results"></div>